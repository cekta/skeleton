{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "cekta/framework": "@dev"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Runtime\\": "runtime/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "./framework"
    }
  ],
  "config": {
    "optimize-autoloader": true
  }
}
